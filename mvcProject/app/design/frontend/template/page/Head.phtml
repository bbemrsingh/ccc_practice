 <!-- This code is generating HTML script and style taÄgs based on the values returned by the `getJs()` and `getCss()` methods. -->
<?php $js = $this->getJs(); ?>
<?php if(count($js)):?>
    <?php foreach($js as $_js):?>
        <script type="" src="<?php echo $_js;?>"></script>
    <?php endforeach;?>
    <?php endif?>

<?php $css = $this->getCss(); ?>
<?php if(count($css)):?>
    <?php foreach($css as $_css):?>
        <style type="" src="<?php echo $_css;?>" ></style>
    <?php endforeach;?>
<?php endif?>